{
  "name": "dhk-align-frontend",
  "version": "2.0.0",
  "private": true,
  "description": "WRAITH Translation Frontend - Clean, modular React app",
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "dev": "vite",
    "preview": "vite preview --port 5173",
    "cra:build": "vite build",
    "start": "vite",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview --port 5173"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2022": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.28.4",
    "@eslint/js": "9.38.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "9.38.0",
    "globals": "^16.4.0",
    "vite": "^7.1.11"
  },
  "start": "vite",
  "build": "vite build",
  "preview": "vite preview --port 5173",
  "vite:dev": "vite",
  "vite:build": "vite build",
  "vite:preview": "vite preview --port 5173"
}
